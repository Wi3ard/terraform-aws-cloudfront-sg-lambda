  },
  "devDependencies": {
    "@serverless/typescript": "^2.28.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/node": "^14.14.31",
    "serverless": "^2.28.0",
    "serverless-offline": "^6.8.0",
    "serverless-webpack": "^5.3.5",
    "shx": "^0.3.3",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
